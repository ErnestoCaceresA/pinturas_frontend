{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch React",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/src",
            "sourceMaps": true,
            "trace": true,
            "resolveSourceMapLocations": [
                "${workspaceFolder}/src/**",
                "!${workspaceFolder}/node_modules/**"
            ]
        }
    ]
}